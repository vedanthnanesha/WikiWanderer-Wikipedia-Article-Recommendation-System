{
  "manifest_version": 3,
  "name": "Wikipedia Similar Articles",
  "description": "Find similar articles on Wikipedia",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*.wikipedia.org/*"],
      "js": ["content.js"]
    }
  ]
}
